cmake -D CMAKE_INSTALL_PREFIX=~/Desktop/opencv/ -D CMAKE_C_FLAGS="--Wno-unused" -D CMAKE_CXX_FLAGS="-g -Wno-unused" -D CUDA_FAST_MATH=ON -D WITH_CUDA=ON -D WITH_HALIDE=ON -D WITH_FFMPEG=ON -D BUILD_opencv_python=OFF -D BUILD_opencv_python3=ON -D OPENCV_ENABLE_NONFREE=ON -D OPENCV_EXTRA_MODULES_PATH=~/Desktop/work/opencv/opencv_contrib/modules/ -D OPENCV_FORCE_PYTHON_LIBS=ON -D CMAKE_TOOLCHAIN_FILE=../platforms/android/android.toolchain.cmake -D ANDROID_NDK=~/Program/android-ndk-r14b/ -D CMAKE_BUILD_TYPE=Release -D ANDROID_ABI=armeabi -D ANDROID_NATIVE_API_LEVEL=21 -D ANDROID_SDK_ROOT=~/Program/sdk/ -D BUILD_ANDROID_EXAMPLES=ON -D BUILD_ANDROID_PROJECTS=OFF -D FFMPEG_INCLUDE_DIRS=/usr/local/lib/pkgconfig/ ..


